void insertSort(int* a, int n) {
	for (int i = 1; i < n; i++) {
		int key = a[i];
		int j = i - 1;
		while (j >= 0 && a[j] > key)
			a[j + 1] = a[j--];
		a[j + 1] = key;
	}
}

void setSort(int* a, int n) {
	for (int i = 0; i < n - 1; i++) {
		int j0 = i;
		for (int j = i + 1; j < n; j++)
			if (a[j] < a[j0])
				j0 = j;
		int temp = a[i];
		a[i] = a[j0];
		a[j0] = temp;
	}
}
